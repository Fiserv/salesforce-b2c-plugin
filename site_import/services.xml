<?xml version="1.0" encoding="UTF-8"?>
<services xmlns="http://www.demandware.com/xml/impex/services/2014-09-26">
    <service-credential service-credential-id="CommercehubCredentials">
        <url>https://[CH_ENVIRONMENT_BASE]/ch/payments-vas/v1/security/credentials</url>
        <user-id></user-id>
        <password/>
    </service-credential>
    <service-credential service-credential-id="CommercehubTokenization">
        <url>https://[CH_ENVIRONMENT_BASE]/ch//payments-vas/v1/tokens</url>
        <user-id></user-id>
        <password/>
    </service-credential>
    <service-credential service-credential-id="CommercehubCharges">
        <url>https://[CH_ENVIRONMENT_BASE]/ch/payments/v1/charges</url>
        <user-id></user-id>
        <password/>
    </service-credential>
    <service-credential service-credential-id="CommercehubCancel">
        <url>https://[CH_ENVIRONMENT_BASE]/ch/payments/v1/cancel</url>
        <user-id></user-id>
        <password/>
    </service-credential>
    <service-credential service-credential-id="CommercehubRefund">
        <url>https://[CH_ENVIRONMENT_BASE]/ch/payments/v1/refunds</url>
        <user-id></user-id>
        <password/>
    </service-credential>

    <service-profile service-profile-id="Fiserv">
        <timeout-millis>30000</timeout-millis>
        <cb-enabled>false</cb-enabled>
    </service-profile>

    <service service-id="CommercehubCredentials">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>fiserv</log-prefix>
        <comm-log-enabled>true</comm-log-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>Fiserv</profile-id>
        <credential-id>CommercehubCredentials</credential-id>
    </service>
    <service service-id="CommercehubTokenization">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>fiserv</log-prefix>
        <comm-log-enabled>true</comm-log-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>Fiserv</profile-id>
        <credential-id>CommercehubTokenization</credential-id>
    </service>
    <service service-id="CommercehubCharges">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>fiserv</log-prefix>
        <comm-log-enabled>true</comm-log-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>Fiserv</profile-id>
        <credential-id>CommercehubCharges</credential-id>
    </service>
    <service service-id="CommercehubCancel">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>fiserv</log-prefix>
        <comm-log-enabled>true</comm-log-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>Fiserv</profile-id>
        <credential-id>CommercehubCancel</credential-id>
    </service>
    <service service-id="CommercehubRefund">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>fiserv</log-prefix>
        <comm-log-enabled>true</comm-log-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>Fiserv</profile-id>
        <credential-id>CommercehubRefund</credential-id>
    </service>
</services>