                </preference>
                <preference preference-id="CommerceHubGiftFormSecurityCodePlaceholderCharacter">
                    <value>•</value>
                </preference>
                <preference preference-id="CommerceHubGiftFormSecurityCodeMask">
                    <value>true</value>
                </preference>
                <preference preference-id="CommerceHubGiftFormSecurityCodeMaskCharacter">
