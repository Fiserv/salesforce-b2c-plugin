    <payment-processor processor-id="FISERV_COMMERCEHUB_GIFT">
        <description>Fiserv Commerce Hub gift card processor</description>
        <preferences>
            <all-instances>

                <!--THIS IS WHERE THE GENERAL SETTINGS START-->

                <preference preference-id="CommerceHubAPIEnvironment">
                    <value>CERT</value>
                </preference>
                <preference preference-id="CommerceHubLogLevel">
                    <value>L2</value>
                </preference>
                <preference preference-id="CommerceHubTimeout">
                    <value>30</value>
                </preference>

                <!--THIS IS WHERE THE GIFT CARD SETTINGS START-->

                <preference preference-id="CommerceHubGiftEnable">
                    <value>false</value>
                </preference>
                <preference preference-id="CommerceHubGiftPaymentMethodTitle">
                    <value>Gift Cards</value>
                </preference>
                <preference preference-id="CommerceHubGiftPaymentType">
                    <value>AUTH</value>
